{
	"name": "billboard.ui",
	"version": "1.0.0",
	"description": "Armani media agency design system",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/lib.umd.cjs",
	"module": "./dist/lib.js",
	"exports": {
		".": {
			"import": "./dist/lib.js",
			"require": "./dist/lib.umd.cjs"
		}
	},
	"private": false,
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"prepare": "husky install",
		"lint": "eslint --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
	},
	"prettier": {
		"arrowParens": "avoid",
		"tabWidth": 2,
		"singleQuote": true,
		"useTabs": true,
		"trailingComma": "all",
		"printWidth": 120,
		"semi": true
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write --ignore-unknown .",
			"eslint ./src --ext .ts,.tsx --fix --quiet"
		]
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-docgen": "^5.4.3",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@storybook/addon-a11y": "^6.5.10",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-essentials": "^6.5.10",
		"@storybook/addon-interactions": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/builder-vite": "^0.2.2",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/react": "^6.5.10",
		"@storybook/testing-library": "^0.0.13",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/styled-components": "^5.1.25",
		"@typescript-eslint/eslint-plugin": "^5.32.0",
		"@typescript-eslint/parser": "^5.32.0",
		"@vitejs/plugin-react": "^2.0.0",
		"antd": "4.18.2",
		"babel-loader": "^8.2.5",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"storybook-addon-designs": "^6.3.1",
		"styled-components": "^5.3.5",
		"typescript": "^4.6.4",
		"vite": "^3.0.0",
		"vite-tsconfig-paths": "^3.5.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alirezaaminii/billboard.ui.git"
	},
	"keywords": [
		"ui-kit",
		"design-system"
	],
	"author": "Alireza Amini <aminicss1@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/alirezaaminii/billboard.ui/issues"
	},
	"homepage": "https://github.com/alirezaaminii/billboard.ui#readme"
}
