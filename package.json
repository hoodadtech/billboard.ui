{
  "name": "billboard.ui",
  "version": "1.0.0",
  "description": "Armani media agency design system",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs"
    }
